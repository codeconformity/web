/* Bootstrap extensions
-------------------------------------------------- */

.navbar-fixed-left {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  border: 0;
  border-radius: 0;
  overflow: hidden;
  margin: 0;
  padding: 0;
  background: transparent;
}
.navbar-fixed-left .navbar-header {
  float: none;
}
.navbar-fixed-left .navbar-collapse {
  padding: 0;
  overflow-x: visible;
  border: 0;
}
.navbar-fixed-left .navbar-brand {
  float: none;
}
.navbar-fixed-left .navbar-nav {
  float: none;
}
.navbar-fixed-left .navbar-nav > li {
  float: none;
}
.navbar-fixed-left .navbar-nav > li > a {
  padding: 15px 24px;
  color: #ffffff;
  opacity: 0.5;
  border-bottom: 1px solid rgba(255,255,255,0.1);
}
.navbar-fixed-left .navbar-nav > li > a:hover {
  opacity: 1;
}
.navbar-fixed-left .navbar-bottom {
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
}
.navbar-fixed-left .navbar-bottom > li > a {
  border-top: 1px solid rgba(255,255,255,0.1);
}

.navbar-fixed-left .navbar-toggle {}
.navbar-fixed-left .nav-collapse.collapse {}

